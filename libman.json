{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "twitter-bootstrap@3.4.1",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap-theme.css",
        "css/bootstrap-theme.css.map",
        "css/bootstrap-theme.min.css",
        "css/bootstrap.css",
        "css/bootstrap.css.map",
        "css/bootstrap.min.css",
        "js/bootstrap.js",
        "fonts/glyphicons-halflings-regular.eot",
        "fonts/glyphicons-halflings-regular.svg",
        "fonts/glyphicons-halflings-regular.ttf",
        "fonts/glyphicons-halflings-regular.woff",
        "fonts/glyphicons-halflings-regular.woff2"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "js/bootstrap-datepicker.js",
        "css/bootstrap-datepicker3.css",
        "css/bootstrap-datepicker3.min.css",
        "locales/bootstrap-datepicker.bg.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/font-awesome.css",
        "css/font-awesome.min.css",
        "fonts/fontawesome-webfont.eot",
        "fonts/fontawesome-webfont.svg",
        "fonts/fontawesome-webfont.ttf",
        "fonts/fontawesome-webfont.woff",
        "fonts/fontawesome-webfont.woff2",
        "fonts/FontAwesome.otf"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_bg.js",
        "localization/messages_bg.min.js",
        "additional-methods.js",
        "additional-methods.min.js",
        "jquery.validate.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "provider": "cdnjs",
      "library": "lightgallery@1.6.12",
      "destination": "wwwroot/lib/lightgallery/"
    }
  ]
}